{
  "name": "budparr.com",
  "version": "1.0.0",
  "description": "Personal Site of Bud Parr",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm-run-all clean -p build:*",
    "build:11ty": "eleventy --quiet",
    "build:rollup": "rollup -c",
