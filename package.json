    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "autoprefixer": "^9.8.6",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "hasha": "^5.2.0",
    "lazysizes": "^5.2.2",
    "npm-run-all": "^4.1.5",
    "postcss-hash": "^2.0.0",
    "postcss-import": "^12.0.1",
    "quicklink": "^2.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.27.1",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.2",
    "tailwindcss": "^1.8.8"
